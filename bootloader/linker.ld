OUTPUT_FORMAT("binary")

ENTRY(_start)

SECTIONS
{
    . = 0x7c00;
    .text :
    {
        *(.text)
    }
    .rodata :
    {
        *(.rodata)
    }
    .data :
    {
        *(.data)
    }
    .bss :
    {
        *(.bss)
        *(COMMON)
    }
    /DISCARD/ : { *(.eh_frame) *(.note.GNU-stack) }
}
